		<xs:sequence>
			<xs:element name="LoyaltyCardId" type="xs:long"/>
			<xs:element name="LoyaltyBrandId" type="xs:long"/>
			<xs:element name="MembershipNumber" type="xs:string"/>
			<xs:element name="ExtensionPoint" type="ExtensionPoint" minOccurs="0"/>						
